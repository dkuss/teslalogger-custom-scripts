          "format": "table",
          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  DATE_FORMAT(StartDate, '%M %Y') as 'Month',\n  sum(km_diff) AS 'km gefahren',\n  sum(consumption_kWh) AS 'kWh verbraucht',\n  (sum(consumption_kWh) / sum(km_diff)) * 1000 AS 'wH / km',\n  sum(UNIX_TIMESTAMP(EndDate) - UNIX_TIMESTAMP(StartDate)) / (60 * 60) as 'Std. gefahren',\n  avg(outside_temp_avg) AS ' avg. Temp'\nFROM trip\nWHERE\n  $__timeFilter(StartDate)\nGROUP BY Month\nORDER BY StartDate DESC",
          "refId": "A",
          "select": [
