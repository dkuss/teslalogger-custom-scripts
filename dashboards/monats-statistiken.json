              }
            ]
          },
          {
            "format": "table",
            "group": [],
            "metricColumn": "none",
            "rawQuery": true,
            "rawSql": "SELECT\n  DATE_FORMAT(StartDate, '%M %Y') as 'Month',\n  sum(charging.charge_energy_added) AS 'kWh geladen',\n  sum(UNIX_TIMESTAMP(EndDate) - UNIX_TIMESTAMP(StartDate)) / (60 * 60) as 'Std. geladen',\n  sum(charging.charge_energy_added) * 0.28 as 'Preis pro 28 ct',\n  sum(charging.charge_energy_added) * 0.39 as 'Preis pro 39 ct'\n  FROM charging\nLEFT JOIN chargingstate ON chargingstate.EndChargingID = charging.id\nWHERE\n  $__timeFilter(chargingstate.StartDate) and TIMESTAMPDIFF(MINUTE, chargingstate.StartDate, chargingstate.EndDate) > 3 and chargingstate.EndChargingID - chargingstate.StartChargingID > 4\nGROUP BY Month\nORDER BY StartDate DESC",
            "refId": "E",
